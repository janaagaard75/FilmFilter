    "license": "MIT",
    "bugs": {
        "url": "https://github.com/TeaMeow/vuepack/issues"
    },
    "homepage": "https://github.com/TeaMeow/vuepack#readme",
    "dependencies": {
        "vue": "2.0.1",
        "vue-router": "2.0.0",
        "vuex": "2.0.0",
        "vuex-router-sync": "3.0.0"
    },
    "devDependencies": {
        "autoprefixer": "6.4.0",
        "babel-core": "6.17.0",
        "babel-loader": "6.2.4",
        "babel-preset-es2015": "6.13.2",
