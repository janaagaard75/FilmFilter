{
  "name": "film-filter-website",
  "version": "0.0.0",
  "description": "Interface for movies scraped on kino.dk.",
  "repository": "https://github.com/janaagaard75/film-filter.git",
  "author": {
    "name": "Jan Aagaard",
    "email": "jan@aagaard.net"
  },
  "license": "ISC",
  "scripts": {
    "build:dev": "yarn install && rimraf dist && webpack -d",
    "build:prod": "yarn install && rimraf dist && cross-env NODE_ENV=production webpack",
    "build": "yarn run build:dev",
    "deploy": "yarn build:prod && firebase deploy",
    "fix": "tslint --fix src/**/*.ts src/**/*.tsx",
    "lint:sass": "sass-lint --verbose --no-exit",
    "lint:typescript:main": "tslint src/main/**/*.ts src/main/**/*.tsx",
    "lint:typescript:workers": "tslint src/workers/**/*.ts",
    "lint:typescript": "yarn run lint:typescript:main && yarn run lint:typescript:workers",
    "lint": "yarn run lint:sass && yarn run lint:typescript",
    "serve": "http-server dist -p 9000",
    "start": "yarn install && webpack-dev-server"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "classnames": "2.2.5",
    "font-awesome": "4.7.0",
    "lz-string": "git+https://github.com/janaagaard75/lz-string",
    "mobx": "3.1.9",
    "mobx-react": "4.1.8",
    "mobx-react-devtools": "4.2.13",
    "moment": "2.18.1",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/classnames": "2.2.0",
    "@types/react": "15.0.24",
    "@types/react-dom": "15.5.0",
    "babili": "0.0.12",
    "babili-webpack-plugin": "0.0.11",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "5.0.0",
    "css-loader": "0.28.1",
    "exports-loader": "0.6.4",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "firebase-tools": "3.8.0",
    "html-webpack-plugin": "2.28.0",
    "http-server": "0.10.0",
    "json-loader": "0.5.4",
    "node-sass": "4.5.3",
    "rimraf": "2.6.1",
    "sass-lint": "1.10.2",
    "sass-loader": "6.0.5",
    "style-loader": "0.17.0",
    "ts-loader": "2.0.3",
    "tslint": "5.2.0",
    "tslint-eslint-rules": "4.0.0",
    "tslint-react": "3.0.0",
    "typescript": "2.3.2",
    "url-loader": "0.5.8",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5",
    "worker-loader": "0.8.0"
  }
}
