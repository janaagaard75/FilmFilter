{
  "name": "film-filter-website",
  "version": "0.0.0",
  "description": "Interface for movies scraped on kino.dk.",
  "repository": "https://github.com/janaagaard75/film-filter.git",
  "author": {
    "name": "Jan Aagaard",
    "email": "jan@aagaard.net"
  },
  "license": "ISC",
  "scripts": {
    "build:dev": "yarn install && rimraf dist && webpack -d",
    "build:prod": "yarn install && rimraf dist && cross-env NODE_ENV=production webpack",
    "build": "yarn run build:dev",
    "deploy": "yarn build:prod && firebase deploy",
    "fix": "tslint --fix src/**/*.ts src/**/*.tsx",
    "lint:sass": "sass-lint --verbose --no-exit",
    "lint:typescript:main": "tslint src/main/**/*.ts src/main/**/*.tsx",
    "lint:typescript:workers": "tslint src/workers/**/*.ts",
    "lint:typescript": "yarn run lint:typescript:main && yarn run lint:typescript:workers",
    "lint": "yarn run lint:sass && yarn run lint:typescript",
    "serve": "http-server dist -p 9000",
    "start": "yarn install && webpack-dev-server"
  },
  "dependencies": {
    "bootstrap": "4.1.3",
    "classnames": "2.2.6",
    "font-awesome": "4.7.0",
    "lz-string": "git+https://github.com/janaagaard75/lz-string",
    "mobx": "5.0.3",
    "mobx-react": "5.2.3",
    "mobx-react-devtools": "6.0.2",
    "moment": "2.22.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "@types/classnames": "2.2.6",
    "@types/react": "16.4.7",
    "@types/react-dom": "16.0.6",
    "babel-minify": "0.4.3",
    "babel-minify-webpack-plugin": "0.3.1",
    "copy-webpack-plugin": "4.5.2",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "exports-loader": "0.7.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "firebase-tools": "4.0.2",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "json-loader": "0.5.7",
    "node-sass": "4.9.2",
    "rimraf": "2.6.2",
    "sass-lint": "1.12.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.21.0",
    "ts-loader": "4.4.2",
    "tslint": "5.11.0",
    "tslint-eslint-rules": "5.3.1",
    "tslint-react": "3.6.0",
    "typescript": "3.0.1",
    "url-loader": "1.0.1",
    "webpack": "4.16.4",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5",
    "worker-loader": "2.0.0"
  }
}
