    "lodash": "4.16.6",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "redux": "3.6.0",
    "redux-actions": "0.13.0",
    "serve-static": "1.11.1",
    "todomvc-app-css": "2.0.6",
    "yargs": "6.3.0"
  },
